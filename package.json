{
  "name": "car-hire-middleware",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:mw": "tsx watch src/index.ts",
    "dev:agent": "cd ../gloriaconnect_test_backend/agent-backend && npm run dev",
    "dev:source": "cd ../gloriaconnect_test_backend/source-backend && npm run dev",
    "dev:all": "concurrently -n MW,AGENT,SOURCE -c green,cyan,magenta \"npm run dev:mw\" \"npm run dev:agent\" \"npm run dev:source\"",
    "build": "tsc -p tsconfig.json && node scripts/fix-imports.cjs",
    "start": "node dist/index.js",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx src/data/seed.ts",
    "seed:unlocode": "tsx src/data/seed-unlocode.ts",
    "test:grpc": "node test-grpc-setup.js",
    "verify": "tsx src/services/verificationRunner.ts",
    "test:db": "tsx scripts/fix-database.js",
    "check:env": "node scripts/check-env.js",
    "verify:admin": "tsx scripts/verify-admin.js",
    "test:login": "node scripts/test-login.js",
    "check:prisma": "node scripts/check-prisma-env.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.7.15",
    "@prisma/client": "^5.18.0",
    "@types/multer": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.9",
    "p-limit": "^7.1.1",
    "pino": "^9.3.2",
    "pino-http": "^11.0.0",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.12",
    "@types/nodemailer": "^6.4.15",
    "@types/pino-http": "^5.8.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "concurrently": "^9.2.1",
    "node-cron": "^4.2.1",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  }
}
