{
  "name": "@carhire/nodejs-sdk",
  "version": "1.0.0",
  "description": "Agent SDK for Car-Hire Middleware (REST + gRPC)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "proto:gen": "mkdir -p src/proto && protoc --proto_path=../../protos --ts_out=src/proto --grpc_out=src/proto --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin ../../protos/*.proto"
  },
  "keywords": [
    "car-hire",
    "sdk",
    "middleware",
    "booking",
    "availability"
  ],
  "author": "",
  "license": "proprietary",
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.15",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}

