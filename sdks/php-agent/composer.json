{
  "name": "hmstech/carhire-php-sdk",
  "description": "Agent SDK for Car-Hire Middleware (REST + gRPC)",
  "type": "library",
  "license": "proprietary",
  "autoload": { "psr-4": { "HMS\\CarHire\\": "src/" } },
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.9",
    "google/protobuf": "^3.25",
    "grpc/grpc": "^1.60"
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "scripts": {
    "proto:gen": "PROTO_DIR=${PROTO_DIR:-../../protos}; mkdir -p src/Proto && protoc --proto_path=$PROTO_DIR --php_out=src/Proto --grpc_out=src/Proto --plugin=protoc-gen-grpc=$(command -v grpc_php_plugin) $PROTO_DIR/*.proto",
    "test": "phpunit"
  }
}



